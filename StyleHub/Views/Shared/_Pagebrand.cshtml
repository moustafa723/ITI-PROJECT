@model (string id ,string name,int count)
<div class="brand-item">
    <div class="form-check">
        <input class="form-check-input" type="checkbox" id="@Model.id">
        <label class="form-check-label" for="@Model.id">
            @Model.name
            <span class="brand-count">(@Model.count)</span>
        </label>
    </div>
</div>

